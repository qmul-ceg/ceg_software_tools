#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/CEG_Encrypted.dmg/CEG/ceg_tools/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.27.4_@jest+transform@30.0.2_@jest+types@30.0.1_babel-jest@_be6f4b39ab839cb0e2cf40bcae391e9b/node_modules/ts-jest/node_modules:/Volumes/CEG_Encrypted.dmg/CEG/ceg_tools/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.27.4_@jest+transform@30.0.2_@jest+types@30.0.1_babel-jest@_be6f4b39ab839cb0e2cf40bcae391e9b/node_modules:/Volumes/CEG_Encrypted.dmg/CEG/ceg_tools/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/CEG_Encrypted.dmg/CEG/ceg_tools/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.27.4_@jest+transform@30.0.2_@jest+types@30.0.1_babel-jest@_be6f4b39ab839cb0e2cf40bcae391e9b/node_modules/ts-jest/node_modules:/Volumes/CEG_Encrypted.dmg/CEG/ceg_tools/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.27.4_@jest+transform@30.0.2_@jest+types@30.0.1_babel-jest@_be6f4b39ab839cb0e2cf40bcae391e9b/node_modules:/Volumes/CEG_Encrypted.dmg/CEG/ceg_tools/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
